<UserControl x:Class="ClienteMarWPFWin7.UI.Modules.EnLinea.EnLinea"
        xmlns:behaviours="clr-namespace:ClienteMarWPFWin7.UI.Views.AttachedProperties"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ClienteMarWPFWin7.UI.Modules.EnLinea"
        mc:Ignorable="d"
        Height="450" Width="800"      
        >
    <Grid>
        <WebBrowser behaviours:WebBrowserHelper.Url="{Binding Url}"
                    HorizontalAlignment="Center"></WebBrowser>
    </Grid>
</UserControl>
